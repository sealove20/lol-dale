{
    "root": true,
    "plugins": ["@typescript-eslint"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/vue3-recommended",
      "prettier",
      "plugin:prettier-vue/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "settings": {
        "prettier-vue": {
          "SFCBlocks": {
            "template": false
          }
        }
      },
      "rules": {
        "prettier-vue/prettier": [
          "error",
          {
            "printWidth": 100,
            "singleQuote": true,
          }
        ]
      }
  }